{
  "root": true,
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "sourceType": "module"
  },
  "globals": {
  },
  "rules": {
    "no-var" : "error",
    "prefer-const": "error",
    "no-undef": 0
  }
}
